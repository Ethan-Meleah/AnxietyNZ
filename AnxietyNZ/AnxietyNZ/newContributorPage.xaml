<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AnxietyNZ.newContributorPage">
    <ContentPage.Content>
        <StackLayout Margin="10, 10, 10, 0">

            <Entry x:Name="image_entry" Placeholder="placeholder for image entry" />
            <Entry x:Name="name_entry"  Placeholder="Contributor Name" />
            <Entry x:Name="email_entry" Placeholder="Contributor Email" />
            <Entry x:Name="ph_number_entry" Placeholder="Contributor Phone Number" />
            <Entry x:Name="address_entry" Placeholder="Contributor Address" />

            <Button Text="Save" Clicked="Save_Contributor" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>